{
  "name": "trae-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "kill-ports": "sh -c 'lsof -ti tcp:3001 | xargs kill -9 2>/dev/null || true; lsof -ti tcp:4001 | xargs kill -9 2>/dev/null || true; lsof -ti tcp:4002 | xargs kill -9 2>/dev/null || true; lsof -ti tcp:4007 | xargs kill -9 2>/dev/null || true'",
    "up": "npm run kill-ports && npm run dev",
    "start": "npm run deploy:prod",
    "start:prod": "npm run deploy:prod",
    "deploy:prod": "npx prisma db push && npx tsx scripts/import_workflow_definitivo.ts && npx tsx api/server.ts",
    "db:cleanup-clinic": "npx prisma db execute --schema=prisma/schema.prisma --file=prisma/cleanup_clinic.sql",
    "start:dev": "./scripts/start-server.sh",
    "start:win": "scripts\\start-server.bat",
    "start:node": "node scripts/start-server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "migrate:procedures": "tsx scripts/migrate_procedure_general_to_ortopedica.ts",
    "update:workflow:railway": "tsx scripts/update_workflow_railway.ts",
    "sync:workflow:railway": "tsx scripts/sync_workflow_to_railway.ts",
    "sync:workflow:railway:upload": "tsx scripts/sync_workflow_to_railway_upload.ts",
    "fix:duplicate-edges": "tsx scripts/fix_duplicate_edges.ts",
    "check:workflow:railway": "tsx scripts/check_workflow_railway.ts",
    "fix:patient-found-flow": "tsx scripts/fix_patient_found_flow.ts",
    "cleanup:workflows": "tsx scripts/cleanup_old_workflows.ts",
    "test:gpt-models": "tsx scripts/test_gpt_models.ts",
    "seed:procedure-template": "tsx scripts/seed_procedure_template.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@tanstack/react-query": "^5.90.9",
    "@types/dagre": "^0.7.53",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@xyflow/react": "^12.9.3",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dagre": "^0.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.0",
    "ffmpeg-static": "^5.3.0",
    "framer-motion": "^12.23.24",
    "helmet": "^8.0.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "openai": "^6.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "recharts": "^2.15.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.0.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^22.15.30",
    "@types/pg": "^8.15.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/supertest": "^6.0.3",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "postcss": "^8.5.3",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
